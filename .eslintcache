      setValue(el.value)\n    }\n\n    return <> <input ref = {inputRef}/>\n        <button onClick={save}>save</button>\n        - actual value {value} </>\n\n}\n\nexport const ControlledInput = () => {\n    const [parentValue, setParentValue] = useState<string>('')\nconst onChange = (e: ChangeEvent<HTMLInputElement>) => {setParentValue(e.currentTarget.value) }\n    return <input value={parentValue} onChange={onChange}/>\n\n}\n\nexport const ControlledCheckbox = () => {\n    const [parentValue, setParentValue] = useState<boolean>(true)\n    const onChange = (e: ChangeEvent<HTMLInputElement>) => {setParentValue(e.currentTarget.checked) }\n    return <input type='checkbox' checked={parentValue} onChange={onChange}/>\n\n}\n\nexport const ControlledSelect = () => {\n    const [parentValue, setParentValue] = useState<string | undefined>('1')\n    const onChange = (e: ChangeEvent<HTMLSelectElement>) => {setParentValue(e.currentTarget.value) }\n    return <select value={parentValue} onChange={onChange}>\n        <option>none</option>\n        <option value={'1'}>Minsk</option>\n        <option value={'2'}>Vilnius</option>\n        <option value={'3'}>Prague</option>\n\n\n    </select>\n\n}\n\n\nexport const ControlledInputWithFixedValue = () => <input value={'it-incubator'}/>;\n","/Volumes/data/React/my-app arch/my-app/src/components/Select/Select.stories.tsx",["178","179"],"import React, {useMemo, useState} from \"react\";\nimport {ItemType, Select} from \"./Select\";\n\n\nexport default {\n    title: 'Select',\n    component: Select,\n}\n\nexport const WithValue = () => {\n    const [value1, setValue1] = useState('1');\n    const [value2, setValue2] = useState('1');\n    const [value3, setValue3] = useState('2');\n    const [counter, setCounter] = useState(0);\n    console.log('Help to ReactMemo');\n\n    const [cities, setCities] = useState<Array<ItemType>>([\n        {value: \"1\", title: 'Minsk', countryID: 1, peopleBL: 2},\n        {value: \"2\", title: 'New-York', countryID: 5, peopleBL: 8},\n        {value: \"3\", title: 'London', countryID: 4, peopleBL: 10},\n        {value: \"4\", title: 'Vilnius', countryID: 2, peopleBL: 0.5},\n        {value: \"5\", title: 'Moscow', countryID: 6, peopleBL: 12},\n    ]);\n\n    const newCities1 = useMemo(() => {\n        return cities.filter(c => c.countryID === 1)\n    }, [cities])\n\n    const newCities2 = useMemo(() => {\n        return cities.filter(c => c.title.toLowerCase().indexOf('m') > -1)\n    }, [cities])\n    const newCities3 = useMemo(() => {\n        return cities.filter(c => c.peopleBL >= 10)\n    }, [cities])\n\n\n    return <>\n        CountryID = 1: <Select value={value1} onChange={setValue1}\n                               items={newCities1}/>\n        With m: <Select value={value2} onChange={setValue2}\n                        items={newCities2}/>\n        More than 10 people: <Select value={value3} onChange={setValue3}\n                                     items={newCities3}/>\n        <button onClick={() => setCounter(counter + 1)}>+</button>\n        {counter}\n    </>\n\n\n}\nexport const WithoutValue = () => {\n    const [value, setValue] = useState(null);\n    return <>\n        <Select\n            onChange={setValue}\n            value={value}\n            items={[\n                {value: \"1\", title: 'Minsk', countryID: 1, peopleBL: 2},\n                {value: \"2\", title: 'New-York', countryID: 5, peopleBL: 8},\n                {value: \"3\", title: 'London', countryID: 4, peopleBL: 10},\n            ]}/>\n    </>\n}","/Volumes/data/React/my-app arch/my-app/src/components/Select/Select.tsx",[],"/Volumes/data/React/my-app arch/my-app/src/components/Accordion/UncontrolledAccordionReducer.tsx",[],"/Volumes/data/React/my-app arch/my-app/src/stories/ReactMemo.stories.tsx",["180"],"import React from 'react';\nimport {useState} from \"react\";\n\nexport default {\n    title:'React.memo demo'\n}\n\nconst NewMessagesCounter = (props: any) => {\n    return <div>{props.count}</div>\n}\n\nconst UsersSecret = (props: {users: Array<string>} ) => {\n    return <div >{props.users.map((u,i) => <div key = {i}>{u}</div>)}</div>\n}\n\nconst Users = React.memo(UsersSecret); // реакт не будет перерисовывать Users при изменении стейта Counter\n\n\nexport const Example1 = () => {\n    const [counter, setCounter] = useState(0);\n    const [users, setUsers] = useState(['Lisa', 'Anna', 'Sophy']);\n    const addUser = ()=> {\n        const newUsers = [...users, 'Sveta' + new Date().getTime()]\n        setUsers(newUsers)\n    }\n\n    return(\n        <>\n            <button onClick={() => setCounter(counter+1)}>+</button>\n            <button onClick= {addUser}>Add user</button>\n            <NewMessagesCounter count = {counter}/>\n            <Users users={users}/>\n        </>\n    )\n\n}","/Volumes/data/React/my-app arch/my-app/src/stories/UseMemo.stories.tsx",["181","182"],{"ruleId":"183","replacedBy":"184"},{"ruleId":"185","replacedBy":"186"},{"ruleId":"187","severity":1,"message":"188","line":5,"column":8,"nodeType":"189","messageId":"190","endLine":5,"endColumn":13},{"ruleId":"187","severity":1,"message":"191","line":33,"column":6,"nodeType":"189","messageId":"190","endLine":33,"endColumn":24},{"ruleId":"187","severity":1,"message":"192","line":36,"column":10,"nodeType":"189","messageId":"190","endLine":36,"endColumn":19},{"ruleId":"183","replacedBy":"193"},{"ruleId":"185","replacedBy":"194"},{"ruleId":"187","severity":1,"message":"195","line":1,"column":16,"nodeType":"189","messageId":"190","endLine":1,"endColumn":24},{"ruleId":"183","replacedBy":"196"},{"ruleId":"185","replacedBy":"197"},{"ruleId":"198","severity":1,"message":"199","line":6,"column":1,"nodeType":"200","endLine":9,"endColumn":2},{"ruleId":"187","severity":1,"message":"201","line":3,"column":9,"nodeType":"189","messageId":"190","endLine":3,"endColumn":14},{"ruleId":"187","severity":1,"message":"202","line":3,"column":16,"nodeType":"189","messageId":"190","endLine":3,"en